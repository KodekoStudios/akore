{
	"name": "akore",
	"version": "2.1.3",
	"description": "Powerful Transpiler maker for JavaScript/TypeScript",
	"type": "module",
	"main": "./lib/index.cjs",
	"exports": {
		".": {
			"import": "./lib/index.mjs",
			"require": "./lib/index.cjs"
		}
	},
	"imports": {
		"@common/*": {
			"import": "./lib/common/*.mjs",
			"require": "./lib/common/*.cjs"
		},
		"@core/*": {
			"import": "./lib/core/*.mjs",
			"require": "./lib/core/*.cjs"
		}
	},
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "biome check --write src/ && unbuild",
		"format": "biome check --write src/",
		"test": "bun test",
		"prepare": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/KodekoStudios/akore.git"
	},
	"bugs": {
		"url": "https://github.com/KodekoStudios/akore/issues"
	},
	"keywords": [
		"transpiler"
	],
	"author": "Kodeko Studios",
	"license": "Kodeko Studios Proprietary License",
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@types/bun": "1.1.10",
		"@types/lodash": "4.17.9",
		"@types/node": "22.7.4",
		"mitata": "1.0.10",
		"unbuild": "2.0.0"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"dependencies": {
		"lodash": "4.17.21"
	}
}
